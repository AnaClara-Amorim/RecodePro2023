Algoritmo "semnome"

Var
   media, n1, n2: real
   i: inteiro


Inicio
   i <- 1

   para i de 1 ate 10 faca

      Escreval ("Escreva a primeira nota: ")
      leia (n1)
      Escreval ("Escreva a segunda nota: ")
      leia (n2)

      media <- (n1 + n2) / 2

      Escreval ("Média ", i, ":", media)

   fimpara



Fimalgoritmo