Algoritmo "Ex01"

Var
// Declaração de variável
nome: caractere
salario, idade: real

Inicio

// Entrada de dados
   salario <- 1000
   idade <- 35
   nome <- "Flavio Mota"
// Saída de dados
   escreval ("O salario do " , nome, " é: ", salario)
   escreval ("A idade do " , nome, " é: ", idade)

Fimalgoritmo